<resources>

    <!-- Base application theme. -->
        <!-- Customize your theme here. -->
    <style name="AppTheme" parent="QMUI.Compat.NoActionBar">
    <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>

        <item name="QMUITopBarStyle">@style/QDTopBar</item>
    </style>


    <style name="StartTheme" parent="AppTheme">
        <item name="android:windowFullscreen">true</item>
        <item name="android:windowBackground">@drawable/img_transition_default</item>

    </style>

    <!-- Application theme. -->
    <style name="recyclerDivider" parent="AppTheme">
        <item name="android:listDivider">@drawable/divider_bg</item>
    </style>


    <style name="QDTopBar" parent="QMUI.TopBar">
        <item name="qmui_topbar_bg_color">@color/white</item>
        <item name="qmui_topbar_title_color">@color/qmui_config_color_white</item>
        <item name="qmui_topbar_subtitle_color">@color/qmui_config_color_white</item>
        <item name="qmui_topbar_height">@dimen/topBarHeight</item>
        <item name="qmui_topbar_image_btn_height">@dimen/topBarHeight</item>
        <item name="qmui_topbar_left_back_drawable_id">@mipmap/icon_back</item>
    </style>

